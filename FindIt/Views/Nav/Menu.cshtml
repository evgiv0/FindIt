@model IEnumerable<FindIt.Models.Category>

@Html.ActionLink("Все категории", "List", "Notice",null, new { @class = "menu_links" })

@foreach (var link in Model)
{
    @Html.RouteLink(link.CategoryName, new
        {
            controller = "Notice",
            action = "List",
            filter = link.CategoryNameEng
        },
        new {@class = "menu_links"}

    )
}